<style type="text/css">
            	
/* TextboxList sample CSS */
ul.holder { margin: 0; border: 1px solid #999; overflow: hidden; height: auto !important; height: 1%; padding: 4px 5px 0; }
*:first-child+html ul.holder { padding-bottom: 2px; } * html ul.holder { padding-bottom: 2px; } /* ie7 and below */
ul.holder li { float: left; list-style-type: none; margin: 0 5px 4px 0; }
ul.holder li.bit-box, ul.holder li.bit-input input { font: 11px "Lucida Grande", "Verdana"; }
ul.holder li.bit-box { 
  -moz-border-radius: 6px; 
  -webkit-border-radius: 6px; 
  border-radius: 6px; 
  /*border: 1px solid #CAD8F3; */
  border: 1px solid #6ea416; 
  /* background: #DEE7F8; */
  background: #bfdfc6; 
  padding: 1px 5px 2px; 
}
ul.holder li.bit-box-focus { 
  border-color: #598BEC; 
  background: #598BEC; 
  color: #fff; 
}
ul.holder li.bit-input input { width: 150px; margin: 0; border: none; outline: 0; padding: 3px 0 2px; } /* no left/right padding here please */
ul.holder li.bit-input input.smallinput { width: 20px; }

/* Facebook demo CSS */      
/*#add { border: 1px solid #999; width: 550px; margin: 50px; padding: 20px 30px 10px; }
*/form ol { font: 11px "Lucida Grande", "Verdana"; margin: 0; padding: 0; }
form ol li.input-text { margin-bottom: 0px; list-style-type: none; padding-bottom: 0px; }
form ol li.input-text label { font-weight: bold; cursor: pointer; display: block; font-size: 13px; margin-bottom: 10px; }
form ol li.input-text input { width: 292px; padding: 5px 5px 6px; font: 11px "Lucida Grande", "Verdana"; }
form ul.holder { width: 292px; background: #FFFFFF; border: solid 1px #D0D0D0; }

#forward-who-list ul.holder li.bit-box,
#quick-who-list ul.holder li.bit-box, 
#who-list ul.holder li.bit-box, 
#apple-list ul.holder li.bit-box { 
  padding-right: 15px; position: relative; 
}

#apple-list ul.holder li.bit-input { margin: 0; }
#apple-list ul.holder li.bit-input input.smallinput { width: 5px; }
ul.holder li.bit-hover { background: #BBCEF1; border: 1px solid #6D95E0; }
ul.holder li.bit-box-focus { 
  border-color: #598BEC; background: #598BEC; color: #fff; }
ul.holder li.bit-box a.closebutton { position: absolute; right: 4px; top: 5px; display: block; width: 7px; height: 7px; font-size: 1px; background: url('/images/meetingwave/buttons/textboxlist.close.gif'); }
ul.holder li.bit-box a.closebutton:hover { background-position: 7px; }
ul.holder li.bit-box-focus a.closebutton, ul.holder li.bit-box-focus a.closebutton:hover { background-position: bottom; }

/* Autocompleter */
#forward-who-auto-results<%= meeting.id %>, 
#quick-who-auto-results<%= meeting.id %>, 
#who-auto-results<%= meeting.id %> { 
  display: none; 
  position: absolute; 
  width: 304px; 
  background: #eee;  
  z-index:1101; 
}


#forward-who-auto-results<%= meeting.id %> .default, 
#quick-who-auto-results<%= meeting.id %> .default, 
#who-auto-results<%= meeting.id %> .default { 
  padding: 5px 7px; border: 1px solid #ccc; border-width: 0 1px 1px; 
}

#forward-who-auto-results<%= meeting.id %> ul, 
#quick-who-auto-results<%= meeting.id %> ul, 
#who-auto-results<%= meeting.id %> ul { 
  display: none; margin: 0; padding: 0; overflow: auto; 
}

#forward-who-auto-results<%= meeting.id %> ul li, 
#quick-who-auto-results<%= meeting.id %> ul li, 
#who-auto-results<%= meeting.id %> ul li { 
  padding: 5px 12px; 
  z-index: 1101; 
  cursor: pointer; 
  margin: 0; 
  list-style-type: none; 
  border: 1px solid #ccc; 
  border-width: 0 1px 1px; 
  font: 11px "Lucida Grande", "Verdana"; 
}

#forward-who-auto-results<%= meeting.id %> ul li em, 
#quick-who-auto-results<%= meeting.id %> ul li em, 
#who-auto-results<%= meeting.id %> ul li em { 
  font-weight: bold; font-style: normal; background: #ccc; 
}

#forward-who-auto-results<%= meeting.id %> ul li.auto-focus, 
#quick-who-auto-results<%= meeting.id %> ul li.auto-focus, 
#who-auto-results<%= meeting.id %> ul li.auto-focus { 
  background: #6EA416; 
  color: #fff; 
}

#forward-who-auto-results<%= meeting.id %> ul li.auto-focus em, 
#quick-who-auto-results<%= meeting.id %> ul li.auto-focus em, 
#who-auto-results<%= meeting.id %> ul li.auto-focus em { 
  background: none; 
}

#demo ul.holder li.bit-input input { padding: 2px 0 1px; border: 1px solid #999; }
#add a { color: #666; }
#add-test { width: 100px; padding: 2px; }
#button_container { margin-left: 70px; margin-bottom: 30px;}

#quick-who-auto-field<%= meeting.id %> { width: 350px; }
#quick-who-auto-results<%= meeting.id %>, #quick-who-auto-results<%= meeting.id %> .feed { width: 362px; } 
#quick-who-auto-results<%= meeting.id %> .default { width: 346px; } 
#quick-who-list ul.holder { width: 350px; }
                
                
                
</style> 